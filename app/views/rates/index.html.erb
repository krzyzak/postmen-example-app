<h1>Listing Rates</h1>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% @rates.each do |rate| %>
      <tr>
        <td><%= rate.id %></td>
        <td><%= rate.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>
